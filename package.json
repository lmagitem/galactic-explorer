{
  "name": "galactic-explorer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "watch:galactic-scanner": "cd ../galactic-scanner/ && cargo watch -x run",
    "dev": "vite",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./",
    "build": "tsc && vite build --emptyOutDir",
    "build:watch": "chokidar './src/**/*.{js,jsx,ts,tsx,css,html,json}' -c 'npm run build'",
    "start:fullstack": "concurrently 'npm run build:watch' 'npm run watch:galactic-scanner'"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@tippyjs/react": "^4.2.6",
    "mathjs": "^11.6.0",
    "object-hash": "^3.0.0",
    "pixi-viewport": "^4.38.0",
    "pixi.js": "^6.5.8",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "redux-thunk": "^2.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/object-hash": "^3.0.2",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "babel-loader": "^8.3.0",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.0"
  }
}
